{
  "name": "notsofast extension",
  "version": "1.0",
  "description": "Check user's current URL",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab"
  ],
  "action": {

  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script-test.js"]
    }
  ]
}
